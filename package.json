{
  "name": "ui-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:stage": "ng build --configuration stage --aot --build-optimizer --vendor-chunk=true",
    "build:beta": "ng build --configuration beta --aot --build-optimizer --vendor-chunk=true",
    "build:prod": "ng build --prod --aot --build-optimizer --vendor-chunk=true",
    "optimizecss": "gulp --gulpfile optimize-css.gulpfile.js",
    "cdn:prod": "gulp --gulpfile cdn-prod.gulpfile.js",
    "cdn:beta": "gulp --gulpfile cdn-beta.gulpfile.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p tsconfig.app.json",
    "sonar": "sonar-scanner",
    "build:stats": "ng build --prod --aot --build-optimizer --vendor-chunk=true --statsJson=true",
    "build:analyze": "webpack-bundle-analyzer dist/uiApp/stats-es2015.json"
  },
  "private": true,
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  },
